<!--pages/search/search.wxml-->

<view wx:for="{{goods_list.data}}" wx:key="_id">
    <van-card
  num="1"
   tag="{{ item.params.g_type }}"
  price="{{ item.params.price }}"
  desc="{{ item.params.description }}"
  title="{{ item.params.title }}"
  thumb="{{ item.params.pic_url[0] }}" thumb-mode
/> </view>